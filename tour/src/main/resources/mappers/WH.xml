"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.oracle.tour.dto">
	<select id="WHLogin" parameterType="Member" resultType="int">
		select count(*) from member where m_id=#{m_id} and m_password=#{m_password}
	</select>
	
